  <section class="contact" id="contact">
    <div class="container-fluid">
      <div class="row">

        <div class="col-lg-7 col-md-6 contact-form-txt">
          <form action="#" method="post">
            <%=form_for @masks do |f| %>
            <div class="form-group contact-forms">
              <%= f.text_field :name,:maxlength=>"24",placeholder:'Name',class: "form-control"%>
              </div>
            <div class="form-group contact-forms">
             <%= f.email_field :email,:maxlength=>"24",placeholder:'E-Mail',class: "form-control"%>
            </div>
            <div class="form-group contact-forms">
             <%= f.text_field :phone_number,:maxlength=>"24",placeholder:'Phone Number',class: "form-control"%>
            </div>
            <div class="form-group contact-forms">
             <%= f.text_field :message,:maxlength=>"24",placeholder:'Message',class: "form-control"%>
            </div>
            <%= f.submit"Send",class: "btn btn-dark"%></p>
          </form>
          <%end%>
